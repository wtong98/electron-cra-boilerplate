{
  "name": "electron-cra-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "main": "public/electron.js",
  "author": {
    "name": "Your Name",
    "email": "your.email@awesome.io",
    "url": "pure.awesomeness@awesome.io"
  },
  "build": {
    "appId": "io.awesome.supermachine",
    "productName": "SuperMachine",
    "copyright": "Copyright Â© 2019 ${author}",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --linux deb tar.xz",
    "postinstall": "electron-builder install-app-deps",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test-react": "react-scripts test",
    "clean": "rm -rf dist build"
  },
  "homepage": "./",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^6.0.4",
    "electron-builder": "^21.2.0"
  }
}
